---
export interface Props {
  href?: string
}

const { href } = Astro.props
---

<script type="speculationrules">
  {
    "prefetch": [
      {
        "where": { "href_matches": "/*" },
        "eagerness": "eager"
      }],
    "prerender": [
      {
        "where": { "href_matches": "/*" },
        "eagerness": "moderate"
      }]
  }
</script>