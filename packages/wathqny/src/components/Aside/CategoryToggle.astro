---
import { ArrowSvg } from 'wathqny:assets'

interface Props {
  name: string
  key: number
}

const { name, key } = Astro.props as Props
---

<button class="wtq-focusable wtq-flex-center wtq-rounded wtq-category-toggle">
  {name}
  <ArrowSvg class="wtq-arrow-icon wtq-motion" width={24} height={24} />
</button>
