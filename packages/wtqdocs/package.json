{
  "name": "wtqdocs",
  "type": "module",
  "version": "0.2.10",
  "description": "My awesome Astro theme created using `astro-theme-provider`!",
  "license": "MIT",
  "homepage": "http://wathqny.pages.dev",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spices-solutions/"
  },
  "bugs": {
    "url": "https://github.com/spices-solutions/"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-integration"
  ],
  "exports": {
    ".": {
      "import": "./index.ts"
    },
    "./plugin": "./integration.ts",
    "./layout/home": "./pages/home.astro",
    "./page/injected.css": "./pages/assets/injected-styles.css.ts",
    "./page/search.json": "./pages/search.json.js",
    "./page/rss.xml": "./pages/rss.xml.js",
    "./page/robots.txt": "./pages/robots.txt.ts",
    "./page/docs": "./pages/docs.astro",
    "./page/docs/categories": "./pages/docs/[...categories].astro",
    "./page/docs/documents": "./pages/docs/[...documents].astro",
    "./page/blog": "./pages/blog.astro",
    "./page/blog/pages": "./pages/blog/[...page].astro",
    "./page/blog/posts": "./pages/blog/[...posts].astro",
    "./page/404": "./pages/404.astro",
    "./schema": "./schema.ts",
    "./blog": "./blog.ts",
    "./styles": "./styles/wathqny.scss",
    "./styles/home": "./styles/pages/_home.scss",
    "./styles/blog": "./styles/pages/_blog.scss",
    "./styles/categories": "./styles/pages/_categories.scss",
    "./styles/block": "./styles/pages/_code.scss",
    "./actions": "./actions.ts"
  },
  "module": "./index.ts",
  "dependencies": {
    "@astrojs/mdx": "^4.1.0",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/ts-plugin": "^1.10.4",
    "@inox-tools/custom-routing": "^0.3.3",
    "@rehype-pretty/transformers": "^0.13.2",
    "@shikijs/transformers": "^3.1.0",
    "@shikijs/types": "^3.1.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vite-pwa/astro": "^0.5.0",
    "@zokki/astro-minify": "^1.1.0",
    "astro": "^5.4.2",
    "astro-auto-import": "^0.4.4",
    "astro-compressor": "^0.4.1",
    "astro-meta-tags": "^0.3.1",
    "astro-min": "^1.3.1",
    "astro-purgecss": "^5.2.2",
    "astro-theme-provider": "^0.7.1",
    "astro-toolkit": "workspace:*",
    "purgecss": "^6.0.0",
    "sass": "^1.85.1",
    "sharp": "0.32.6",
    "sharp-ico": "^0.1.5",
    "shiki": "^3.1.0",
    "typescript": "^5.8.2"
  }
}
