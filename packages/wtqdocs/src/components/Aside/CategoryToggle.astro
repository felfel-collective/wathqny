---
import Svg from '@cxa/astro-inline-svg'
import arrowSvg from '../../assets/icons/arrow.svg?raw'
interface Props {
  name: string
}

const { name } = Astro.props as Props
---



<button class="wtq-focusable wtq-flex-center wtq-rounded wtq-category-toggle">
  {name}
  <Svg class="wtq-arrow-icon wtq-motion"  raw={arrowSvg} />
</button>
