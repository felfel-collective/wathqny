---
import ArrowSvg from "../../assets/icons/arrow.svg";

interface Props {
  name: string;
  key: number;
}

const { name, key } = Astro.props as Props;
---

<button class="wtq-focusable wtq-flex-center wtq-rounded wtq-category-toggle">
  {name}
  <ArrowSvg class="wtq-arrow-icon wtq-motion" size={24} />
</button>
