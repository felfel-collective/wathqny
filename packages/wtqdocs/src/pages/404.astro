---
import BaseLayout from '../components/Layout/BaseLayout.astro'
import 'wtqdocs/styles/home'

type Props = {
  title: string
  subtitle: string
  description: string
  sorry: string
  action: string
}

---

<BaseLayout title={"404"}>
  <main id="main-content">
    <section class="hero">
      <h1 class="title">404</h1>
      <p>page not found
        <a
          data-astro-prefetch
          style="cursor: pointer;"
          onclick="window.history.back()"
        > go back</a>
      </p>
    </section>
  </main>
</BaseLayout>
